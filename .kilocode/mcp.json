{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      }
    },
    "filesystem": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--mount",
        "type=bind,src=//wsl.localhost/Ubuntu-22.04/home/loadcosmos,dst=/projects",
        "mcp/filesystem",
        "/projects"
      ]
    }
  }
}